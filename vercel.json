{
  "version": 2,
  "name": "zk-pret-core-engine",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/api/gleif/(.*)",
      "dest": "/api/gleif.js"
    },
    {
      "src": "/api/corporate/(.*)",
      "dest": "/api/corporate.js"
    },
    {
      "src": "/api/exim/(.*)",
      "dest": "/api/exim.js"
    },
    {
      "src": "/api/risk/(.*)",
      "dest": "/api/risk.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "functions": {
    "api/**.js": {
      "runtime": "@vercel/node@3",
      "memory": 1024,
      "maxDuration": 300
    }
  },
  "env": {
    "NODE_ENV": "production",
    "BUILD_ENV": "TESTNET"
  }
}